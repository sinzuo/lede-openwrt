get_device_info_wan_current_ip_addr() {
local val=""
local mode=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} get pifii.global.mode 2> /dev/null`
if [ "$mode" = "ap" ]; then
	val=`ifconfig | grep br-lan -A 1 | grep inet | cut -d ':' -f2 | cut -d ' ' -f1|head -1`
else
	val=`ifconfig | grep eth0.2 -A 1 | grep inet | cut -d ':' -f2 | cut -d ' ' -f1`
fi
freecwmp_output "InternetGatewayDevice.DeviceInfo.wan_current_ip_addr" "$val"
}

