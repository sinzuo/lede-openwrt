if [ "$val" == "mt7603e" ]; then
   val=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} get wireless.@wifi-iface[${i}].ssid 2> /dev/null`
   break;
fi     	
done 
freecwmp_output "InternetGatewayDevice.DeviceInfo.wireless_ssid" "$val"
}

