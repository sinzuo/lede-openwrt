set_device_info_white_list1() {
if [ "$1" != "N/A" ]; then 
/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} set wifidog.wifidog.whitelist="$1"
else
/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} delete wifidog.wifidog.whitelist
fi
/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} commit wifidog.wifidog
echo 1 > /tmp/freecwmp_wireless_need_restart.tmp
}

