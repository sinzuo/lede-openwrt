   break;                                                                        
  fi

  if [ "$val" == "ra0" ]; then
   if [ $second == 3 ]; then 
    val2=`/sbin/uci ${UCI_CONFIG_DIR:+-c $UCI_CONFIG_DIR} get wireless.@wifi-iface[${i}].ssid 2> /dev/null`
    break;
   fi
   let second+=1
  fi 
    	
done	                                                                                               
